public class CC_1_1 {
    public boolean uniqueChar(String input) {
        boolean[] ascii = new boolean[256];
        if(input == null) {
            return false;
        }

        char[] charArray = input.toCharArray();

        for(int i = 0; i < charArray.length; i++) {
            int index = charArray[i];
            if(ascii[index]) {
                return false;
            } else {
                ascii[index] = true;
            }
        }

        return true;
    }

    public static void main(String args[]) {
        CC_1_1 c = new CC_1_1();
        String input = "";          // true
        String input1 = " ";        // true
        String input2 = "  ";       // false
        String input3 = "aaaaa";    // false
        String input4 = "abcde";    // true
        String input5 = "abcda";    // false

        System.err.println(input + ": " + c.uniqueChar(input));
    }
}
